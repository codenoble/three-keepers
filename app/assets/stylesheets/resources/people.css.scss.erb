body.people.index {
  ul.people.media-list li {
     width: 380px;
     height: 150px;
     margin-top: 20px;
     margin-right: 40px;
     float: left;
  }
}

body.people.show {
  table.photos {
    td.photo img.zoomable {
      max-height: <%= Settings.views.photos.max_height %>px;
      cursor: -webkit-zoom-in;
      cursor: -moz-zoom-in;
    }
  }
}
